package edu.ucsc.leeps.fire.cong.client.gui;

public class FPoint {

    public float x, y;
    public boolean visible;

    public FPoint() {
        x = 0;
        y = 0;
        visible = true;
    }

    public FPoint(float X, float Y) {
        x = X;
        y = Y;
        visible = true;
    }
}
